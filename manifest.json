{
    "name": "Picture Transfer",
    "version": "1.0",
    "manifest_version": 2,
    "icons":
    {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "<all_urls>"
    ],
    "content_scripts": [
    {
        "matches": [
            "https://twitter.com/*",
            "https://www.pixiv.net/*",
            "https://www.lofter.com/*"
        ],
        "css": [
            "css/jquery-ui.min.css",
            "css/fileTranfer.css"
        ],
        "js": [
            "js/jquery-3.3.1.min.js",
            "js/jquery-ui.min.js",
            "js/endpoint.js",
            "js/fileTranfer.js",
            "js/pictureSearch.js",
            "js/imgContentManager.js"
        ],
        "run_at": "document_end"
    }],
    "background":
    {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    }
}